(define (fizzbuzz n) 
 (if (equal? (remainder n 3) 0)
       (word 'fizz)
       (+ n)))
       
       
 (if (equal? (remainder n 5) 0)
     (word 'buzz)
     (* 1 n))
 (if (equal? (remainder n 5) 0) and (equal? (remainder n 3) 0)
     (word 'fizzbuzz)
     (* 1 n)))
   

